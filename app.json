{
  "expo": {
    "name": "Mockback: Companion App",
    "slug": "mockback",
    "scheme": "mockback",
    "version": "0.1.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.tusharyaar.mockback",
      "versionCode": 1,
      "permissions":[]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Companion mobile app for Mockback."
  }
}
